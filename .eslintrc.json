{
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended",
    "prettier",
    "prettier/flowtype",
    "react-app",
    "prettier/react"
  ],
  "plugins": [
    "prettier",
    "flowtype"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "no-unexpected-multiline": 2,
    "import/prefer-default-export": 0,
    "prettier/prettier": 2,
    "jsx-a11y/href-no-hash": 0,
    "func-names": 0
  }
}
